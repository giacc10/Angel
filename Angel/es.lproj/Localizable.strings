"Phrase−Peace−1" = "La paz sea contigo. Deja ir tus preocupaciones y confía en el viaje que tienes por delante.";
"Phrase−Peace−2" = "Recuerda, eres amado y protegido, siempre.";
"Phrase−Peace−3" = "Respira hondo y suelta toda la tensión de tu cuerpo. Deja que la paz te llene.";
"Phrase−Peace−4" = "Cierra los ojos e imagina una luz cálida y brillante que te rodea. Esta es la luz de la paz y el amor.";
"Phrase−Peace−5" = "Piensa en todas las cosas buenas de tu vida y deja que te llenen de gratitud y paz.";
"Phrase−Peace−6" = "Recuerda, nunca estás solo. Siempre estoy aquí para guiarte y apoyarte.";
"Phrase−Peace−7" = "Deja ir todos tus miedos y abraza la paz que hay dentro de ti.";
"Phrase−Peace−8" = "Concéntrate en el momento presente y deja de lado cualquier estrés o ansiedad sobre el futuro.";
"Phrase−Peace−9" = "Tómate un tiempo para relajarte y recargar energías. Cuando regreses, te sentirás descansado y en paz.";
"Phrase−Peace−10" = "Imagínese estar rodeado de amor y luz. Permita que ese amor y esa luz le traigan paz y consuelo.";
"Phrase−Peace−11" = "Deja que tu corazón se llene de amor y deja que ese amor te traiga paz.";
"Phrase−Peace−12" = "Da un paso atrás, respira hondo y sé que todo estará bien.";
"Phrase−Peace−13" = "Recuerda ser amable contigo mismo. Deja ir las dudas y abraza la paz.";
"Phrase−Peace−14" = "Visualiza un lugar tranquilo, un lugar donde te sientas seguro y protegido. Quédate allí todo el tiempo que necesites.";
"Phrase−Peace−15" = "Confía en el viaje que tienes por delante. Confía en que todo sucede por una razón.";
"Phrase−Peace−16" = "Tómate el tiempo para reflexionar sobre las cosas buenas de tu vida. Deja que la gratitud te llene de paz.";
"Phrase−Peace−17" = "Deja que la luz dentro de ti brille intensamente y deja que esa luz traiga paz a todos los que te rodean.";
"Phrase−Peace−18" = "Rodéate de amor y positividad. Deja que esa energía te traiga paz y consuelo.";
"Phrase−Peace−19" = "Tómate un momento para quedarte quieto y escuchar tu voz interior. Deja que esa voz te guíe hacia la paz.";
"Phrase−Peace−20" = "Recuerda, eres amado, eres fuerte y eres capaz de encontrar la paz en cualquier situación.";
"Phrase−Peace−21" = "Deja atrás el pasado y concéntrate en el presente. Es en este momento cuando se puede encontrar la paz.";
"Phrase−Peace−22" = "Encuentra alegría en las cosas simples y deja que esa alegría te traiga paz.";
"Phrase−Peace−23" = "Da un paseo por la naturaleza, deja que su belleza y tranquilidad calmen tu alma.";
"Phrase−Peace−24" = "Permítete descansar, porque es en el descanso que se puede restaurar la paz.";
"Phrase−Peace−25" = "Visualízate rodeado de un cálido y reconfortante abrazo. Ese abrazo es el abrazo de la paz.";
"Phrase−Peace−26" = "Respira hondo e imagina que todas tus preocupaciones y miedos se desvanecen.";
"Phrase−Peace−27" = "Deja que el ritmo de tu respiración te traiga paz y serenidad.";
"Phrase−Peace−28" = "Piensa en un recuerdo feliz y permite que ese recuerdo traiga una sonrisa a tu rostro y paz a tu corazón.";
"Phrase−Peace−29" = "Encuentra un lugar tranquilo para sentarte y estar quieto. Deja que la paz te envuelva.";
"Phrase−Peace−30" = "Permítete sentir el amor a tu alrededor. Deja que ese amor te traiga paz.";
"Phrase−Peace−31" = "Piensa en todas las bendiciones de tu vida y sé agradecido. Deja que la gratitud te traiga paz.";
"Phrase−Peace−32" = "Tómate un momento para conectarte con tu ser interior y deja que esa conexión te traiga paz.";
"Phrase−Peace−33" = "Concéntrate en los aspectos positivos de tu vida y deja que esos aspectos positivos te traigan paz.";
"Phrase−Peace−34" = "Permítete perdonar, porque es en el perdón donde se encuentra la paz.";
"Phrase−Peace−35" = "Piensa en alguien que te ame y deja que ese amor te traiga paz.";
"Phrase−Peace−36" = "Tómate un momento para meditar y deja que la quietud de tu mente traiga paz.";
"Phrase−Peace−37" = "Imagina que eres un lago pacífico y tranquilo. Deja que la quietud del lago te traiga paz.";
"Phrase−Peace−38" = "Respira hondo e imagínate rodeado de un aura de paz.";
"Phrase−Peace−39" = "Permítete sentirte satisfecho, porque es en el contentamiento donde se puede encontrar la paz.";
"Phrase−Peace−40" = "Tómate un momento para apreciar todo lo que tienes y deja que esa apreciación te traiga paz.";
"Phrase−Peace−41" = "Permítete sentir el calor del sol y deja que ese calor traiga paz a tu alma.";
"Phrase−Peace−42" = "Piensa en un lugar donde te sientas seguro y protegido, y deja que ese lugar te traiga paz.";
"Phrase−Peace−43" = "Permítete sentir alegría, porque es en la alegría que se puede encontrar la paz.";
"Phrase−Peace−44" = "Concéntrate en el momento presente y deja de lado cualquier ansiedad sobre el futuro.";
"Phrase−Peace−45" = "Tómate un momento para escuchar los sonidos de la naturaleza y deja que esos sonidos traigan paz.";
"Phrase−Peace−46" = "Permítete sentirte tranquilo y centrado. Deja que esa quietud te traiga paz.";
"Phrase−Peace−47" = "Piensa en un momento de tu vida en el que te sentiste en paz y deja que ese recuerdo te devuelva la paz.";
"Phrase−Peace−48" = "Respira hondo e imagínate envuelto en una niebla pacífica.";
"Phrase−Peace−49" = "Permítete sentir esperanza, porque es en la esperanza que se puede encontrar la paz.";
"Phrase−Peace−50" = "Concéntrate en la belleza del mundo que te rodea y deja que esa belleza traiga paz a tu alma.";
"Phrase−Peace−51" = "Permítete sentir la calma que viene con la aceptación.";
"Phrase−Peace−52" = "Recuerda, nunca estás solo. Siempre hay ángeles que te cuidan y te guían hacia la paz.";
"Phrase−Peace−53" = "Tómese un momento para reflexionar sobre sus fortalezas y logros. Deje que esta reflexión le brinde paz y confianza.";
"Phrase−Peace−54" = "Deja ir cualquier culpa o vergüenza. Permite que el perdón te traiga paz y sanación.";
"Phrase−Peace−55" = "Concéntrate en tu respiración y deja que su ritmo te guíe hacia la paz interior.";
"Phrase−Peace−56" = "Piensa en una presencia reconfortante en tu vida e imagina esa presencia contigo ahora, brindándote paz.";
"Phrase−Peace−57" = "Tómate un momento para conectarte con el momento presente y deja que esa conexión te traiga paz.";
"Phrase−Peace−58" = "Permítete sentir el poder de las afirmaciones positivas y deja que su energía te traiga paz y aliento.";
"Phrase−Peace−59" = "Tómate un momento para practicar la gratitud y deja que esta práctica te traiga paz y alegría.";
"Phrase−Peace−60" = "Piensa en un color relajante e imagínalo rodeándote, brindándote paz y calma.";
"Phrase−Peace−61" = "Permítete sentir la libertad que viene con dejar ir. Dejar ir el control, el miedo o la preocupación puede traer paz.";
"Phrase−Peace−62" = "Recuerda ser amable contigo mismo. El autocuidado y la autocompasión pueden traer paz interior y armonía.";
"Phrase−Peace−63" = "Tómate un momento para practicar la atención plena y deja que esa práctica te brinde paz y claridad.";
"Phrase−Peace−64" = "Permítete sentir un sentido de propósito y deja que ese propósito te traiga paz y satisfacción.";
"Phrase−Peace−65" = "Concéntrese en las sensaciones de su cuerpo y deje que esa conciencia le brinde paz y relajación.";
"Phrase−Peace−66" = "Piensa en una imagen pacífica, como un amanecer o una playa, e imagínate en ese lugar, rodeado de paz.";
"Phrase−Peace−67" = "Permítete sentir el poder del diálogo interno positivo. Las palabras de aliento y amor pueden traer paz y confianza.";
"Phrase−Peace−68" = "Tómate un momento para imaginarte flotando en un cuerpo de agua pacífico, rodeado de calma y serenidad.";
"Phrase−Peace−69" = "Recuerda practicar la compasión hacia los demás y deja que esta práctica te traiga paz y armonía.";
"Phrase−Peace−70" = "Permítete sentir una sensación de conexión con el universo y deja que esa conexión te traiga paz y un sentido de pertenencia.";
"Phrase−Peace−71" = "Recuerda que la paz comienza con un solo pensamiento. Concéntrate en los pensamientos positivos y deja que la paz irradie desde adentro.";
"Phrase−Peace−72" = "Deja de lado cualquier juicio hacia ti mismo o hacia los demás. La aceptación y la comprensión pueden traer paz y armonía.";
"Phrase−Peace−73" = "Permítete sentir el poder curativo de la naturaleza. Pasa tiempo al aire libre y deja que su belleza te traiga paz.";
"Phrase−Peace−74" = "Imagina una luz dorada rodeándote, llenándote de calidez y paz.";
"Phrase−Peace−75" = "Recuerde tomar descansos y descansar cuando lo necesite. Nutrir su cuerpo y mente puede traer paz y rejuvenecimiento.";
"Phrase−Peace−76" = "Concéntrate en el momento presente y deja de lado las preocupaciones sobre el pasado o el futuro. Abraza la paz que viene con vivir en el presente.";
"Phrase−Peace−77" = "Permítete sentir el poder del perdón. Perdonarte a ti mismo o a los demás puede traer paz y liberación.";
"Phrase−Peace−78" = "Recuerda que eres capaz de crear paz en cualquier situación. Elige la bondad, la compasión y la comprensión.";
"Phrase−Peace−79" = "Tómese un momento para escribir en un diario sus pensamientos y sentimientos. El acto de escribir puede traer paz y claridad.";
"Phrase−Peace−80" = "Piensa en alguien que te brinde paz e imagínalo enviándote amor y apoyo.";
"Phrase−Peace−81" = "Permítete sentir gratitud por las pequeñas cosas de la vida. Un corazón agradecido puede traer paz y satisfacción.";
"Phrase−Peace−82" = "Concéntrate en el centro de tu corazón e imagínalo brillando con una energía pacífica y amorosa.";
"Phrase−Peace−83" = "Respira hondo e imagínate llenando tu cuerpo de paz. Mientras exhalas, libera cualquier tensión o estrés.";
"Phrase−Peace−84" = "Recuerda que la paz siempre está dentro de ti. Solo tienes que permitirte acceder a ella.";
"Phrase−Peace−85" = "Imagina una brisa suave y relajante que te envuelve y te trae paz y tranquilidad.";
"Phrase−Peace−86" = "Permítete sentir la alegría que viene con estar en el momento presente. Deja que esa alegría te traiga paz y satisfacción.";
"Phrase−Peace−87" = "Recuerde practicar el cuidado personal y deje que esa práctica le brinde paz y una sensación de bienestar.";
"Phrase−Peace−88" = "Tómate un momento para meditar y deja que la práctica te brinde paz y un sentido más profundo de conexión contigo mismo y con el universo.";
"Phrase−Peace−89" = "Imagine un santuario pacífico, donde pueda retirarse cuando lo necesite para encontrar la calma y la serenidad.";
"Phrase−Peace−90" = "Concéntrate en el centro de tu corazón e imagínalo expandiéndose con amor y paz, irradiando hacia el mundo que te rodea.";
"Phrase−Peace−91" = "Permítete sentir una sensación de armonía interior, donde la mente, el cuerpo y el alma están en perfecto equilibrio y paz.";
"Phrase−Peace−92" = "Recuerda que eres digno de paz y felicidad. Permítete recibirlo.";
"Phrase−Peace−93" = "Imagina una luz suave y brillante rodeándote, llenándote de paz y amor.";
"Phrase−Peace−94" = "Tómate un momento para conectarte con tu niño interior y deja que esa conexión te traiga alegría y paz.";
"Phrase−Peace−95" = "Permítete sentir un sentido de propósito y dirección. Deja que ese propósito te guíe hacia la paz y la realización.";
"Phrase−Peace−96" = "Recuerda practicar la paciencia y la comprensión hacia ti mismo y hacia los demás. Deja que esta práctica te traiga paz y compasión.";
"Phrase−Peace−97" = "Imagina un jardín tranquilo, donde puedas descansar y reflexionar siempre que necesites encontrar paz y tranquilidad.";
"Phrase−Peace−98" = "Permítete sentir la libertad que viene con vivir auténticamente. Deja que esa libertad te traiga paz y autoaceptación.";
"Phrase−Peace−99" = "Concéntrate en la belleza y la maravilla del mundo que te rodea. Deja que esta apreciación te traiga paz y asombro.";
"Phrase−Peace−100" = "Recuerda ser amable contigo mismo y con los demás, y deja que esa amabilidad te traiga paz y un sentido más profundo de conexión con la humanidad.";

"Phrase−Hope−1" = "Todas las cosas saldrán bien al final, ten fe.";
"Phrase−Hope−2" = "Sé fuerte y mantén la fe, porque habrá días mejores.";
"Phrase−Hope−3" = "Recuerda, eres amado y apreciado pase lo que pase.";
"Phrase−Hope−4" = "No pierdas la esperanza, porque siempre hay un resquicio de esperanza en cada tormenta.";
"Phrase−Hope−5" = "Eres capaz de superar cualquier obstáculo, solo cree en ti mismo.";
"Phrase−Hope−6" = "Ten coraje y sabe que todo sucede por una razón.";
"Phrase−Hope−7" = "El futuro es brillante, sigan adelante con esperanza y determinación.";
"Phrase−Hope−8" = "Ten coraje, porque nunca te darán un desafío que no puedas cumplir.";
"Phrase−Hope−9" = "Tengan esperanza y fe en el viaje, porque los llevará a donde deben estar.";
"Phrase−Hope−10" = "Siempre hay una luz al final del túnel, sigue caminando hacia ella.";
"Phrase−Hope−11" = "Eres fuerte, resistente y capaz de superar cualquier dificultad.";
"Phrase−Hope−12" = "Mantente enfocado en tus metas y nunca pierdas de vista tus sueños.";
"Phrase−Hope−13" = "Recuerda, las cosas mejorarán, sigue aguantando.";
"Phrase−Hope−14" = "Cada día es una nueva oportunidad para empezar de nuevo y hacer las cosas bien.";
"Phrase−Hope−15" = "El camino puede ser difícil, pero el destino valdrá la pena.";
"Phrase−Hope−16" = "Nunca pierdas la esperanza, porque incluso la noche más oscura eventualmente dará paso a la luz del amanecer.";
"Phrase−Hope−17" = "Cuando la vida se ponga difícil, sigue poniendo un pie delante del otro.";
"Phrase−Hope−18" = "Se te ha dado la fuerza y ​​el coraje para enfrentar cualquier desafío que se te presente.";
"Phrase−Hope−19" = "Ten paciencia y ten fe, porque todo saldrá bien al final.";
"Phrase−Hope−20" = "Nunca te rindas, porque eres un luchador y saldrás victorioso.";
"Phrase−Hope−21" = "Recuerda, siempre hay esperanza, no importa cuán difíciles parezcan las cosas.";
"Phrase−Hope−22" = "Da un paso a la vez y no olvides cuidarte en el camino.";
"Phrase−Hope−23" = "Cuando te sientas perdido, mira hacia el cielo y sabe que no estás solo.";
"Phrase−Hope−24" = "Ten fe en ti mismo y cree que puedes hacer una diferencia en el mundo.";
"Phrase−Hope−25" = "Cada obstáculo es una oportunidad para aprender y fortalecerse.";
"Phrase−Hope−26" = "Mantente fiel a tu corazón y nunca dejes que nadie apague tu chispa.";
"Phrase−Hope−27" = "Tened esperanza y fe en el camino, porque os llevará a grandes cosas.";
"Phrase−Hope−28" = "Nunca dejes que el miedo te detenga, porque tienes el coraje y la determinación para superar cualquier cosa.";
"Phrase−Hope−29" = "Cuando la vida se pone difícil, aférrate a las cosas que más importan.";
"Phrase−Hope−30" = "Nunca estás solo, porque el universo siempre te apoya en tu viaje.";
"Phrase−Hope−31" = "Sigue avanzando, porque cada paso que das te acerca a tus sueños.";
"Phrase−Hope−32" = "Recuerda, el éxito no es un destino, sino un viaje lleno de obstáculos y triunfos.";
"Phrase−Hope−33" = "Nunca pierdas la fe, porque hasta en la noche más oscura brillan las estrellas.";
"Phrase−Hope−34" = "Mantente positivo y nunca te rindas, porque eres capaz de grandes cosas.";
"Phrase−Hope−35" = "Ten esperanza y sigue sonriendo, porque mejores días están a la vuelta de la esquina.";
"Phrase−Hope−36" = "Eres un milagro, lleno de amor, luz y potencial ilimitado.";
"Phrase−Hope−37" = "Cuando la vida te deprima, levántate y sigue adelante.";
"Phrase−Hope−38" = "Mantén tu corazón abierto y nunca dejes que la negatividad te deprima.";
"Phrase−Hope−39" = "Cree en ti mismo y nunca te rindas, porque estás destinado a la grandeza.";
"Phrase−Hope−40" = "Sé fuerte y sigue adelante, porque la esperanza y la felicidad siempre están al alcance de la mano.";
"Phrase−Hope−41" = "Aférrate a la esperanza, porque te llevará a través de los tiempos más oscuros.";
"Phrase−Hope−42" = "Confía en que todo sucede por una razón, incluso si aún no puedes verla.";
"Phrase−Hope−43" = "Cree en ti mismo, porque eres capaz de alcanzar la grandeza.";
"Phrase−Hope−44" = "Cuando las cosas se pongan difíciles, respira hondo y sigue adelante.";
"Phrase−Hope−45" = "Confía en que el universo conspira a tu favor, incluso cuando las cosas parecen inciertas.";
"Phrase−Hope−46" = "Cada nuevo día es una oportunidad para empezar de nuevo y hacer las cosas bien.";
"Phrase−Hope−47" = "Recuerda, nunca te dan un desafío que no puedas manejar.";
"Phrase−Hope−48" = "Mantente fiel a tus valores y nunca cedas en lo que más te importa.";
"Phrase−Hope−49" = "El pasado está detrás de ti, el futuro está frente a ti y el hoy es un regalo, por eso se llama presente.";
"Phrase−Hope−50" = "Cree en el poder del amor y la bondad, porque pueden superar incluso el mayor de los obstáculos.";
"Phrase−Hope−51" = "Cuando te sientas perdido, mira dentro y confía en tu voz interior.";
"Phrase−Hope−52" = "Nunca te rindas, porque incluso los pasos más pequeños pueden conducir a grandes resultados.";
"Phrase−Hope−53" = "Ten fe en que el universo te está conduciendo hacia tu propósito y destino.";
"Phrase−Hope−54" = "Cada desafío es una oportunidad de crecimiento y transformación.";
"Phrase−Hope−55" = "Mantente positivo, porque tus pensamientos y creencias crean tu realidad.";
"Phrase−Hope−56" = "Recuerda que nunca estás realmente solo, ya que siempre hay ángeles cuidándote.";
"Phrase−Hope−57" = "Confía en el poder de la comunidad y la conexión, porque todos estamos juntos en esto.";
"Phrase−Hope−58" = "Ten esperanza y sigue esforzándote para alcanzar tus metas, porque el éxito está a la vuelta de la esquina.";
"Phrase−Hope−59" = "Mantente conectado a tierra en el momento presente, porque es el único momento que realmente importa.";
"Phrase−Hope−60" = "Cree en el poder de tus sueños y la magia de las posibilidades.";
"Phrase−Hope−61" = "Cuando una puerta se cierra, otra siempre se abre: mantén los ojos y el corazón abiertos a nuevas oportunidades.";
"Phrase−Hope−62" = "Nunca dejes que el miedo te detenga, porque es solo una ilusión.";
"Phrase−Hope−63" = "Ten fe en que todo saldrá bien al final, porque el universo está de tu lado.";
"Phrase−Hope−64" = "Recuerda que eres un creador poderoso, capaz de manifestar tus deseos más profundos.";
"Phrase−Hope−65" = "Mantente fiel a tu luz interior, porque eso es lo que te hace único y especial.";
"Phrase−Hope−66" = "Cree en ti mismo, porque eres digno de amor, éxito y felicidad.";
"Phrase−Hope−67" = "Cuando te sientas abrumado, da un paso a la vez.";
"Phrase−Hope−68" = "Ten esperanza y confía en que el universo tiene un plan para ti, incluso si aún no puedes verlo.";
"Phrase−Hope−69" = "Mantente abierto a nuevas posibilidades, ya que pueden llevarte a alegrías y bendiciones inesperadas.";
"Phrase−Hope−70" = "Recuerda que tus luchas no te definen, eres mucho más que tus desafíos.";
"Phrase−Hope−71" = "Cree en el poder del perdón y la compasión, porque pueden curar hasta las heridas más profundas.";
"Phrase−Hope−72" = "Cuando las cosas parezcan imposibles, recuerda que los milagros suceden todos los días.";
"Phrase−Hope−73" = "Confía en que estás exactamente donde se supone que debes estar ahora mismo.";
"Phrase−Hope−74" = "Mantén la curiosidad y la mente abierta, ya que siempre hay algo nuevo por descubrir.";
"Phrase−Hope−75" = "Cree en el poder de tu imaginación, porque puede crear mundos y realidades más allá de tus sueños más salvajes.";
"Phrase−Hope−76" = "Cuando te sientas desanimado, tómate un momento para reflexionar sobre lo lejos que has llegado.";
"Phrase−Hope−77" = "Ten esperanza y mantén tu corazón abierto, porque el amor y los milagros pueden venir de lugares inesperados.";
"Phrase−Hope−78" = "Mantente fiel a tu visión, porque te guiará hacia tu verdadero propósito.";
"Phrase−Hope−79" = "Recuerda que cada desafío que superas te hace más fuerte y resistente.";
"Phrase−Hope−80" = "Cree en tu capacidad para superar cualquier obstáculo y hacer realidad tus sueños.";
"Phrase−Hope−81" = "Cuando tengas ganas de rendirte, recuerda que la persistencia y la determinación son la clave del éxito.";
"Phrase−Hope−82" = "Ten fe en que el universo te está guiando hacia tu mayor bien.";
"Phrase−Hope−83" = "Mantente enfocado en el momento presente, porque ahí es donde reside tu poder.";
"Phrase−Hope−84" = "Cree en el poder de la gratitud y la positividad, porque pueden transformar tu vida.";
"Phrase−Hope−85" = "Cuando te sientas perdido, tómate un momento para conectarte con tu intuición y sabiduría interior.";
"Phrase−Hope−86" = "Ten esperanza y confía en que todo saldrá bien para tu mayor bien, incluso si no lo sientes así en este momento.";
"Phrase−Hope−87" = "Manténgase abierto a la sabiduría de los demás, ya que pueden ofrecer información y orientación valiosas.";
"Phrase−Hope−88" = "Cree en el poder del amor propio y el cuidado personal, porque son esenciales para tu bienestar.";
"Phrase−Hope−89" = "Cuando te sientas abrumado, da un paso atrás y date el tiempo y el espacio que necesitas para recargar.";
"Phrase−Hope−90" = "Ten fe en tu viaje y confía en que todo se está desarrollando exactamente como debería.";
"Phrase−Hope−91" = "Recuerda que tienes la fuerza dentro de ti para superar cualquier desafío.";
"Phrase−Hope−92" = "Creer que cada revés es solo un obstáculo temporal que se puede superar con perseverancia y determinación.";
"Phrase−Hope−93" = "Cuando la vida se ponga difícil, aférrate a la esperanza y sigue adelante con fe y coraje.";
"Phrase−Hope−94" = "Tengan fe en que las cosas mejorarán y confíen en que sus luchas finalmente conducirán a mayores bendiciones.";
"Phrase−Hope−95" = "Manténgase enfocado en lo positivo y deje de lado cualquier negatividad o creencia limitante que lo esté frenando.";
"Phrase−Hope−96" = "Cree en el poder de tu resiliencia y fuerza interior y sabe que puedes superar cualquier cosa que se te presente.";
"Phrase−Hope−97" = "Cuando tengas ganas de rendirte, recuerda que no estás solo y que siempre hay alguien que se preocupa por ti y está dispuesto a ayudarte.";
"Phrase−Hope−98" = "Ten esperanza en el futuro y mantén tu corazón abierto a todas las posibilidades y oportunidades que te esperan.";
"Phrase−Hope−99" = "Manténgase fiel a sus metas y sueños y confíe en que su arduo trabajo y persistencia valdrán la pena al final.";
"Phrase−Hope−100" = "Cree en ti mismo y en tus habilidades, y sabe que eres capaz de lograr cualquier cosa que se te presente.";

"Phrase−Blessing−1" = "Que siempre encuentres paz en tu corazón.";
"Phrase−Blessing−2" = "Que estés rodeado de amor y luz.";
